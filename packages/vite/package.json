    "@babel/types": "^7.19.3",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "execa": "^5.1.1",
    "get-port": "^5.1.1",
    "globby": "11.1.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isarray": "^4.0.0",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1"
  }
